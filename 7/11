namespace Code117
{
    struct School
    {
        public string schName;
        public string stName;
        public int stGrade;

    }
    internal class Program
    {
        static void Main(string[] args)
        {
            School sc;
            sc.schName = "레이크사이드 고등학교";
            sc.stGrade = 100;
            Console.WriteLine(sc.schName);
            
        }
    }
}
