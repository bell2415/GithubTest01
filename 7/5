namespace RandomApp01
{
    internal class Program
    {
        static void Main(string[] args)
        {
            Random random = new Random();

            for (int i = 0; i < 7; i++)
            {
                Console.WriteLine(random.Next(1, 46));
            }
        }
    }
}
